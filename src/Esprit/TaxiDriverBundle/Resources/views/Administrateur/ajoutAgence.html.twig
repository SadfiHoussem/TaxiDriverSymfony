{# empty Twig template #}
  
{% extends "PidevTaxiBundle::layout.html.twig" %}
{% block breadcrumb %}
<ul class="breadcrumb">
				<li>
					<i class="icon-home"></i>
					<a href={{path("esprit_taxidriver_admin_homepage")}}>Accueil</a> 
					<i class="icon-angle-right"></i>
				</li>
				<li><a href="#">Gestion agences</a></li>
